@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix svb: <https://regels.overheid.nl/termen/> .
@prefix mim: <http://modellen.mim-standaard.nl/def/mim#> .
#
#################################################################################
#                                                                               #
# CONCEPTUEEL INFORMATIEMODEL SOCIALE VERZEKERINGSBANK (SVB)                    #
#                                                                               #
#################################################################################
#
svb:Persoon a mim:Objecttype ;
    rdfs:label
        "Mens"@nl ,
        "Human being"@en ;
    rdfs:comment
        "Persoon met vast te stellen en/of vastgestelde pensioengerechtigde leeftijd."@nl,
        "Person with a retirement age to be determined and/or established."@en ;

svb:Leeftijd rdf:type rdf:Property ;
        rdfs:domain svb:Persoon ;
        rdfs:range xsd:integer ;
        rdfs:label
            "Leeftijd"@nl ,
            "Age"@en ;
        rdfs:comment
            "De leeftijd van een persoon."@nl ,
            "The age of a person."@en .

svb:heeftEchtgenoot rdf:type rdf:Property ;
              rdfs:domain svb:Persoon ;
              rdfs:range svb:Persoon ;
              rdfs:label
                "Heeft echtgenoot"@nl ,
                "Has spouse"@en ;
              rdfs:comment
                "Linkt een persoon aan zijn/haar echtgenoot."@nl ,
                "Links a person to their spouse."@en .

svb:AOWleetfijd rdf:type rdfs:Class ;
          rdfs:subClassOf svb:Persoon ;
          rdfs:label
            "Vastgestelde pensioengerechtigde leeftijd"@nl ,
            "Determined retirement age"@nl ;
