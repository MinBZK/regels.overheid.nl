    # 
    ronl:analysis ronl:WetsanalyseJAS ;
    ronl:method ronl:ALEF ;
    #
    dct:creator <https://organisaties.overheid.nl/71117/Directoraat-generaal_Toeslagen> ;
    dct:subject "Sociale zekerheid."@nl ;
    dct:subject "Social security."@en ;
